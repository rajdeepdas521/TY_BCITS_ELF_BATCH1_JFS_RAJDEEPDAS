<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       
        <bean id="employee" class="com.bcits.springcore.beans.EmployeeBean1">
        <property name="name" value="sudhakr"/>
        <property name="age" value="55"/>
        <property name="departmentBean" ref="department"/>
        </bean>
        
        <bean id="department" class="com.bcits.springcore.beans.DepartmentBean">
        <property name="deptId" value="11"/>
        <property name="deptName" value="dev"/>
        
        </bean>
</beans>